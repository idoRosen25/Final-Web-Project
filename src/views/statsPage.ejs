<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous" />
  <link rel="stylesheet" href="../public/css/orders.css">
  <%- include('./partials/head') %>
</head>

<body>
  <div class="container-fluid overflow-hidden text-center">
    <%- include('./partials/header'); %>
      <div class="row">
        <div class="m-auto my-4 col-8 col-xl-6 chartContianer">
          <div class="m-auto col-4">
            <select id="rangeSelect" class="form-select mb-2">
              <option selected value="reset">Select</option>
              <option value="week">Last Week</option>
              <option value="month">Last Month</option>
              <option value="year">Last Year</option>
            </select>
          </div>

          <div id="rangeChart" class=""></div>
        </div>
      </div>

      <div class="row">
        <div class="m-auto my-4 col-8 col-xl-6 chartContianer">
          <div class="m-auto col-4">
            <button id="categoryChartBtn" class="btn btn-primary mb-2">Refresh</button>
          </div>
          <div id="categoryChart" class=""></div>
        </div>
      </div>

      <div class="row">
        <div class="m-auto my-4 col-8 col-xl-6 chartContianer">
          <div class="m-auto col-4">
            <button id="usersChartBtn" class="btn btn-primary mb-2">Refresh</button>
          </div>
          <div id="usersChart" class=""></div>
        </div>
      </div>

      <%- include('./partials/footer'); %>
        <script src="../public/js/graph.js"></script>
  </div>
</body>

</html>

<script src="https://unpkg.com/ag-charts-community/dist/ag-charts-community.min.js"></script>